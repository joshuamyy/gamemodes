    {
        for (new i = 0; i != MAX_ENTRANCES; i ++)
        {
            if(!EntranceData[i][entranceExists])
            {
                EntranceData[i][entranceExists] = true;
                EntranceData[i][entranceIcon] = 0;
                EntranceData[i][entranceType] = 0;
                EntranceData[i][entranceCustom] = 0;
