    SendClientMessageEx(playerid, X11_YELLOW_2, "(( PM to %s (%d): %s ))", ReturnName2(userid, 0), userid, text);

    foreach(new i : Player) if(SQL_IsCharacterLogged(i) && (AccountData[i][pAdmin] >= 5) && !PlayerData[i][pDisableAPM])
