            else SendClientMessageEx(playerid, X11_YELLOW, "[ID: %d] %s %s (%s)", i, gAdminAlias[AccountData[i][pAdmin]], ReturnAdminName(i), NormalName(i));

            count++;
        }
    }
    if(!count) 
            return SendClientMessage(playerid, X11_WHITE, "* No Admin/Helper/Volunteer online.");

    return 1;
}

