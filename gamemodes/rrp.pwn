    new
        name[32];

    for (new i = 0; i < sizeof(g_aFurnitureData); i ++) if(g_aFurnitureData[i][e_FurnitureModel] == model) {
        strcat(name, g_aFurnitureData[i][e_FurnitureName]);

        break;
    }
    return name;
}

