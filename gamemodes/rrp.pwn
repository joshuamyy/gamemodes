                BusinessData[id][bizInt][1] = -30.8741;
                BusinessData[id][bizInt][2] = 1003.5573;
                BusinessData[id][bizInt][3] = 0.0000;
                BusinessData[id][bizInterior] = 4;
            }
            case 2: {
                BusinessData[id][bizInt][0] = 316.3963;
                BusinessData[id][bizInt][1] = -169.8375;
