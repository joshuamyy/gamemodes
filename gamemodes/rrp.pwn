        case 789: if(GetFactionType(playerid) != FACTION_SAGS) return SendErrorMessage(playerid, "This is SAGS freqency only.");
    }
    static
        string[128];

    if(strlen(params) > 64)
    {
        format(string, sizeof(string), "%s: %.64s",ReturnName(playerid, 0), params);
