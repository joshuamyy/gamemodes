IsValidEmail(const email[]) 
{ 
    new at_pos = strfind(email, "@", true) + 1; 
    if(email[0] == '@' || at_pos == -1) 
    { 
        return false; 
    } 

    static const providers[][] = 
    { 
        "hotmail.com", 
        "gmail.com"
    }; 

    for(new i = 0; i < sizeof(providers); i ++) 
    { 
        if(!strcmp(email[at_pos], providers[i], true)) 
        {
            return true; 
        } 
    } 
    return false; 
}  

