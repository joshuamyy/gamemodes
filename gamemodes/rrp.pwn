{
    for(new i = 0; i != sizeof(arrModshop); i++) if(SQL_IsCharacterLogged(playerid) && IsPlayerInRangeOfPoint(playerid, range, arrModshop[i][0], arrModshop[i][1], arrModshop[i][2])) {
        return i;
    }
    return -1;
}

