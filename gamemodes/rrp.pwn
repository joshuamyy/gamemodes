    }

    if(newinteriorid != 0) {
        SetPlayerWeather(playerid, 4);
        SetPlayerTime(playerid, 12, 0);
    }
    else {
        new hour;
        gettime(hour, _, _);
        SetPlayerWeather(playerid, current_weather);
