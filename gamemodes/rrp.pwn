                EntranceData[i][entrancePos][1] = y;
                EntranceData[i][entrancePos][2] = z;
                EntranceData[i][entrancePos][3] = angle;

                EntranceData[i][entranceInt][0] = 0.0000;
                EntranceData[i][entranceInt][1] = 0.0000;
                EntranceData[i][entranceInt][2] = 0.0000;
